<h2>Add Category </h2>

<%= form_for(:category, :url => create_category_path, :html => {:multipart => true}, :method => "get") do |f| %>
<p>
  <label>Category Name</label>
  <%= f.text_field :category_name %>
</p>

<p>
  <label>Category Image</label>
  <%= f.file_field :category_image %>
</p>

<p>
  <%= f.submit :Add  %>
</p>

<% end %>